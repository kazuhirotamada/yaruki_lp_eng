---

import '@/assets/scss/common.scss';
import '@/assets/scss/pc.scss';
import Head from '@/components/head.astro';
import GtagBody from '@/components/gtag/gtm_body.astro';
import Header from '@/components/header/Header.astro';
import Footer from '@/components/footer/Footer.astro';



const pageID = "sec";
const pageSlug = "contact";
---

<html lang="en">
	<Head pageSlug={pageSlug} />	


	<body id={pageID}>
		<GtagBody />

		<Header />
		
		<main>
			

			<section class="unitWrap" id="contactWrap">
				<div class="cont">
					<div class="titleWrap commonH2">
						<p class="bgText">Contact us</p>
						<h2>Contact us</h2>
					</div>

					<link rel="stylesheet" href="/contact/css/form.css">
					
					<div class="thanksWrapper">
						
						<div class="thanks">
							<p>Thank you for contacting us. <br />Your message has been successfully sent. <br />We will get back to you as soon as possible.</p>

							<button  onclick="location.href='/'">Back to Top Page</button>
						</div>
					</div>

				</div>
			</section>



		</main>

		<Footer />

		
		
	</body>
</html>
